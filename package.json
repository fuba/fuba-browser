{
  "name": "fuba-browser",
  "version": "1.0.0",
  "description": "Browser automation tool with REST API for LLM agents",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "docker-compose up",
    "stop": "docker-compose down",
    "logs": "docker-compose logs -f",
    "rebuild": "docker-compose build --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuba/fuba-browser.git"
  },
  "keywords": [
    "browser",
    "automation",
    "rest-api",
    "electron",
    "chromium"
  ],
  "author": "fuba",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/fuba/fuba-browser/issues"
  },
  "homepage": "https://github.com/fuba/fuba-browser#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/chrome-remote-interface": "^0.33.0",
    "@types/node": "^25.0.10",
    "@types/turndown": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "electron-builder": "^26.5.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "chrome-remote-interface": "^0.33.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "electron": "^39.3.0",
    "express": "^5.2.1",
    "puppeteer-core": "^24.35.0",
    "turndown": "^7.2.2"
  }
}
